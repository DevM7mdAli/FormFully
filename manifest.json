{
  "manifest_version": 3,
  "name": "autofill",
  "description": "autofill any website",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  
  "action": {
          "default_popup": "index.html",
          "default_icon": "icon.png"
  },

  "host_permissions": [
    "https://www.developer.chrome.com/*"
  ],
  "optional_host_permissions":[
    "https://*/*",
    "http://*/*"
  ],

  
  "permissions": [
      "activeTab",
      "scripting"
  ]
}