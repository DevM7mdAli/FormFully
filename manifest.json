{
  "manifest_version": 3,
  "name": "FormFully",
  "description": "Automatically Auto fill in input fields with any value you want to save your precious time",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  
  "action": {
          "default_popup": "index.html",
          "default_icon": "icon.png"
  },

  "host_permissions": [
    "https://www.developer.chrome.com/*"
  ],
  
  "permissions": [
      "activeTab",
      "scripting"
  ]
}